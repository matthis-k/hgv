<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<Pane fx:id="renderPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/15.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="kit.pse.hgv.view.uiHandler.RenderHandler">
    <children>
        <Circle fx:id="renderCircle" fill="#eceff4" radius="300.0" stroke="#eceff4" strokeType="INSIDE"
                strokeWidth="0.0"/>
        <CheckBox fx:id="centerCheckBox" mnemonicParsing="false" onAction="#changeCenterVisibility"
                  text="Zentrum sichtbar" textFill="#2e3440"/>
    </children>
</Pane>
